{
  "version": 2,
  "buildCommand": "SKIP_PREFLIGHT_CHECK=true npm run build",
  "outputDirectory": "dist",
  "env": {
    "SKIP_PREFLIGHT_CHECK": "true",
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}